<Project Sdk="Meadow.Sdk/1.1">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <OutputType>Exe</OutputType>
    <AssemblyName>App</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Meadow.Foundation" Version="0.10.5" />
    <PackageReference Include="Meadow.Foundation.Displays.TftSpi" Version="0.6.2" />
    <PackageReference Include="Meadow.Foundation.Displays.GraphicsLibrary" Version="0.8.2" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="trees.bmp" />
  </ItemGroup>
  <ItemGroup>
    <None Update="trees.bmp">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="trees.bmp">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>
</Project>